{
  "name": "@gamifyio/react",
  "version": "0.1.6",
  "description": "React SDK for Gamify event tracking",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --max-warnings 0",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@gamifyio/core": "*"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "framer-motion": ">=10.0.0"
  },
  "peerDependenciesMeta": {
    "framer-motion": {
      "optional": true
    }
  },
  "devDependencies": {
    "@repo/typescript-config": "*",
    "@types/react": "^19.0.0",
    "framer-motion": "^11.0.0",
    "react": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "5.9.2"
  },
  "sideEffects": false,
  "license": "MIT",
  "keywords": [
    "analytics",
    "tracking",
    "events",
    "sdk",
    "react"
  ]
}
